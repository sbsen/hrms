            function show(input) {
                if (input.files && input.files[0]) {
                    var filerdr = new FileReader();
                    filerdr.onload = function (e) {
                        $('#profile_image').attr('src', e.target.result);
                        $("#employeeUploadProfileImage").prop('disabled', false);
