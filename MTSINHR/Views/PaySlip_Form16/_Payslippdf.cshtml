            </td>
        </tr>
       
    </table>
    <table cellspacing="0" cellpadding="10" style="border-collapse: collapse;border: 1px solid #000;border-top:0; width: 100%;">
         <tr>
            <td style="width:30%">

                <table cellspacing="0" cellpadding="0" style="border-collapse: collapse; width: 100%;">
                    <tr>
                        <td style="width: 20%;"><b>Net Pay</b></td>
                        <td style="width: 80%;text-transform:uppercase">&#8377  <b>@Model.Net_Salary &nbsp&nbsp&nbsp <label id="Amountinwords"></label></b></td>
                        
                    </tr>

                </table>
            </td>
         
        </tr>
    </table>
  <script>
      var a = ['', 'one ', 'two ', 'three ', 'four ', 'five ', 'six ', 'seven ', 'eight ', 'nine ', 'ten ', 'eleven ', 'twelve ', 'thirteen ', 'fourteen ', 'fifteen ', 'sixteen ', 'seventeen ', 'eighteen ', 'nineteen '];
      var b = ['', '', 'twenty', 'thirty', 'forty', 'fifty', 'sixty', 'seventy', 'eighty', 'ninety'];
      
      inWords(parseInt(@Model.Amountinwords))
      function inWords(num) {
          
          if ((num = num.toString()).length > 9) return 'Value Exceeded';
          n = ('000000000' + num).substr(-9).match(/^(\d{2})(\d{2})(\d{2})(\d{1})(\d{2})$/);
          if (!n) return; var str = '';
          str += (n[1] != 0) ? (a[Number(n[1])] || b[n[1][0]] + ' ' + a[n[1][1]]) + 'crore ' : '';
          str += (n[2] != 0) ? (a[Number(n[2])] || b[n[2][0]] + ' ' + a[n[2][1]]) + 'lakh ' : '';
          str += (n[3] != 0) ? (a[Number(n[3])] || b[n[3][0]] + ' ' + a[n[3][1]]) + 'thousand ' : '';
          str += (n[4] != 0) ? (a[Number(n[4])] || b[n[4][0]] + ' ' + a[n[4][1]]) + 'hundred ' : '';
          str += (n[5] != 0) ? ((str != '') ? 'and ' : '') + (a[Number(n[5])] || b[n[5][0]] + ' ' + a[n[5][1]]) :'';

          document.getElementById("Amountinwords").innerHTML=str + 'rupees only';
      }
      var i;
              for (i = 0; i < document.getElementsByClassName("tright").length; i++) {
                  if (document.getElementsByClassName("tright")[i].innerHTML == "0.00") {
                      document.getElementsByClassName("tright")[i].parentElement.style.display = "none"
                  }
              }
        
           
   </script>
    
   
</body>
</html>





