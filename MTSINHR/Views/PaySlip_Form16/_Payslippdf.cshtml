                    </tr>
                    <tr>
                        <td style="width: 40%;">PF</td>
                        <td class="tright" style="width: 60%;">@Model.PF</td>
                    </tr>
                    <tr>
                        <td>ESI</td>
                        <td class="tright">@Model.ESI</td>
